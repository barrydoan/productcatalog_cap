<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="cartDialog"
		noDataText="No Products Found"
		title="Select Product"
		search=".onSearch"
		confirm=".onDialogClose"
		cancel=".onDialogClose"
		items="{
			path: '/Carts'
		}">
		<StandardListItem
			title="{CardNo}"
			description="{ID}"
			iconDensityAware="false"
			iconInset="false"
			info="{total}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>